<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Dkackman.MockHttp</id>
    <version>1.0.0</version>
    <title>MockHttp</title>
    <authors>dkackman</authors>
    <owners>dkackman</owners>
    <licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
    <projectUrl>https://github.com/dkackman/MockHttp</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A library to allow mocking of client http logic with System.Net.Http. Supports recording and playback of responses. Particularly useful for separating client-side unit tests from web services.
    </description>
    <summary></summary>
    <releaseNotes>Alpha release</releaseNotes>
    <copyright>Copyright Don Kackman 2015</copyright>
    <language>en-US</language>
    <tags>PCL http REST mock unit testing</tags>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="6.0.6" />
      <dependency id="Microsoft.Net.Http" version="2.2.28" />
      <dependency id="Microsoft.Bcl" version="1.1.9" />
      <dependency id="Microsoft.Bcl.Build" version="1.0.21" />
    </dependencies>
  </metadata>
  <files>
    <!-- desktop files -->
    <file src="..\MockHttp\bin\Release\MockHttp.dll" target="lib\net45" />
    <file src="..\MockHttp\bin\Release\MockHttp.xml" target="lib\net45" />
    <file src="..\MockHttp.Desktop\bin\Release\MockHttp.Desktop.dll" target="lib\net45" />
    <file src="..\MockHttp.Desktop\bin\Release\MockHttp.Desktop.xml" target="lib\net45" />

    <!-- store files -->
    <file src="..\MockHttp\bin\Release\MockHttp.dll" target="lib\portable-netcore45+wpa81" />
    <file src="..\MockHttp\bin\Release\MockHttp.xml" target="lib\portable-netcore45+wpa81" />
    <file src="..\MockHttp.Store\bin\Release\MockHttp.Store.dll" target="lib\portable-netcore45+wpa81" />
    <file src="..\MockHttp.Store\bin\Release\MockHttp.Store.xml" target="lib\portable-netcore45+wpa81" />
  </files>
</package>